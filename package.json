    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.0.0",
    "fs-extra": "^2.1.2",
    "html-webpack-plugin": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.0.0",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.0.2",
